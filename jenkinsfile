pipeline{
    agent any
    stages {
        
        stage("Testing JDK"){
            steps {
                sh "java -version"
            }
        }
        stage("Chekout Git"){
            steps {
                echo "Pulling"
            }
        }
        stage('MVN COMPILE') {
            steps{
                sh 'mvn compile'
        }
}
stage("NEXUS") {
            steps {
                sh "nexus"
            }
        }    
    }
stage("Testing Maven"){
            steps {
                sh "mvn -version"
            }
        }
    

    }
}
